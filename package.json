{
    "name": "manual-screeps",
    "version": "0.1.1",
    "description": "manual-screeps",
    "main": "index.js",
    "//": "If you add or change the names of destinations in screeps.json, make sure you update these scripts to reflect the changes",
    "scripts": {
        "lint": "eslint \"src/**/*.ts\" --fix",
        "build": "rollup -c & typedoc --options ./typedoc.config.json",
        "push-ureium": "rollup -c --environment DEST:u-main",
        "push-moreOneNight": "rollup -c --environment DEST:miniMain",
        "push-pserver": "rollup -c --environment DEST:pserver",
        "push-pserver-more": "rollup -c --environment DEST:pserver-more",
        "push-season": "rollup -c --environment DEST:season",
        "push-sim": "rollup -c --environment DEST:sim",
        "test": "npm run test-unit",
        "test-unit": "mocha test/unit/**/*.ts",
        "test-integration": "echo 'See docs/in-depth/testing.md for instructions on enabling integration tests'",
        "watch-main": "rollup -cw --environment DEST:main",
        "watch-pserver": "rollup -cw --environment DEST:pserver",
        "watch-season": "rollup -cw --environment DEST:season",
        "watch-sim": "rollup -cw --environment DEST:sim",
        "z-push-screepsCc": "rollup -c --environment DEST:screepsCc",
        "ts-coverage": "typescript-coverage-report"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Ureimu/manual-screeps.git"
    },
    "author": "Ureium",
    "license": "Unlicense",
    "bugs": {
        "url": "https://github.com/Ureimu/manual-screeps/issues"
    },
    "homepage": "https://github.com/Ureimu/manual-screeps#readme",
    "engines": {
        "node": "10.x || 12.x"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^11.1.0",
        "@rollup/plugin-node-resolve": "^7.1.3",
        "@rollup/plugin-replace": "^6.0.2",
        "@types/chai": "^4.1.6",
        "@types/lodash": "3.10.2",
        "@types/mocha": "^5.2.5",
        "@types/node": "^13.13.1",
        "@types/screeps": "^3.1.0",
        "@types/sinon": "^5.0.5",
        "@types/sinon-chai": "^3.2.0",
        "@typescript-eslint/eslint-plugin": "^4.31.1",
        "@typescript-eslint/parser": "^4.31.1",
        "@typescript-eslint/typescript-estree": "^4.31.1",
        "chai": "^4.2.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-prettier": "^4.0.0",
        "lodash": "^3.10.1",
        "rollup": "^2.6.1",
        "rollup-plugin-clear": "^2.0.7",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-cpy": "^2.0.1",
        "rollup-plugin-html": "^0.2.1",
        "rollup-plugin-screeps": "^1.0.0",
        "rollup-plugin-typescript2": "^0.27.0",
        "sinon": "^6.3.5",
        "sinon-chai": "^3.2.0",
        "ts-node": "^8.8.2",
        "tsconfig-paths": "^3.9.0",
        "typedoc": "^0.22.0",
        "typedoc-plugin-markdown": "^3.8.0",
        "typescript-coverage-report": "^0.6.0"
    },
    "dependencies": {
        "@rollup/plugin-json": "^4.1.0",
        "cli-diagram": "^0.3.0",
        "js-base64": "^3.6.0",
        "mocha": "^5.2.0",
        "prettier-eslint": "^13.0.0",
        "screeps-calculator": "^0.1.11",
        "screeps-utf15": "^1.0.1",
        "source-map": "~0.6.1",
        "typescript": "^4.2.4"
    }
}
